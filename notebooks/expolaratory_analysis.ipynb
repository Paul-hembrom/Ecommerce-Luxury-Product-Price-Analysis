    "# notebooks/exploratory_analysis.py\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import plotly.express as px\n",
    "import plotly.graph_objects as go\n",
    "from plotly.subplots import make_subplots\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "# Set style\n",
    "plt.style.use('default')\n",
    "sns.set_palette(\"husl\")\n",
    "%matplotlib inline\n",
    "\n",
    "class FarfetchEDA:\n",
    "    def __init__(self, data_path: str = None):\n",
    "        self.df = None\n",
    "        self.load_data(data_path)\n",
    "    \n",
    "    def load_data(self, data_path: str):\n",
    "        \"\"\"Load and prepare data for EDA\"\"\"\n",
    "        if data_path:\n",
    "            self.df = pd.read_parquet(data_path)\n",
    "        else:\n",
    "            # Try to load from default path\n",
    "            try:\n",
    "                self.df = pd.read_parquet(\"../data/processed/products_cleaned.parquet\")\n",
    "            except:\n",
    "                print(\"Please provide the path to your data file\")\n",
    "                return\n",
    "        \n",
    "        print(f\"Dataset loaded: {self.df.shape[0]:,} rows, {self.df.shape[1]} columns\")\n",
    "        print(\"\\nFirst few rows:\")\n",
    "        display(self.df.head())\n",
    "    \n",
    "    def basic_info(self):\n",
    "        \"\"\"Display basic dataset information\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"BASIC DATASET INFORMATION\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        print(f\"Total Products: {len(self.df):,}\")\n",
    "        print(f\"Total Brands: {self.df['brand_clean'].nunique():,}\")\n",
    "        print(f\"Total Categories: {self.df['main_category'].nunique():,}\")\n",
    "        print(f\"Date Range: {self.df.get('date', 'Not available')}\")\n",
    "        \n",
    "        print(\"\\nData Types:\")\n",
    "        print(self.df.dtypes)\n",
    "        \n",
    "        print(\"\\nMissing Values:\")\n",
    "        missing_data = self.df.isnull().sum()\n",
    "        missing_pct = (missing_data / len(self.df)) * 100\n",
    "        missing_df = pd.DataFrame({\n",
    "            'Missing Count': missing_data,\n",
    "            'Missing Percentage': missing_pct\n",
    "        })\n",
    "        display(missing_df[missing_df['Missing Count'] > 0])\n",
    "    \n",
    "    def price_analysis(self):\n",
    "        \"\"\"Analyze price distribution and patterns\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"PRICE ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        # Basic price statistics\n",
    "        price_stats = self.df['price_final_usd'].describe()\n",
    "        print(\"Price Statistics:\")\n",
    "        display(pd.DataFrame(price_stats).T)\n",
    "        \n",
    "        # Create subplots\n",
    "        fig, axes = plt.subplots(2, 3, figsize=(18, 12))\n",
    "        fig.suptitle('Price Distribution Analysis', fontsize=16, fontweight='bold')\n",
    "        \n",
    "        # 1. Price distribution histogram\n",
    "        axes[0, 0].hist(self.df['price_final_usd'], bins=50, alpha=0.7, edgecolor='black')\n",
    "        axes[0, 0].set_xlabel('Price (USD)')\n",
    "        axes[0, 0].set_ylabel('Frequency')\n",
    "        axes[0, 0].set_title('Price Distribution')\n",
    "        axes[0, 0].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 2. Log price distribution (to handle outliers)\n",
    "        log_prices = np.log1p(self.df['price_final_usd'])\n",
    "        axes[0, 1].hist(log_prices, bins=50, alpha=0.7, edgecolor='black')\n",
    "        axes[0, 1].set_xlabel('Log(Price + 1)')\n",
    "        axes[0, 1].set_ylabel('Frequency')\n",
    "        axes[0, 1].set_title('Log Price Distribution')\n",
    "        axes[0, 1].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 3. Price by category (top 10)\n",
    "        top_categories = self.df['main_category'].value_counts().head(10).index\n",
    "        category_prices = self.df[self.df['main_category'].isin(top_categories)]\n",
    "        sns.boxplot(data=category_prices, x='main_category', y='price_final_usd', ax=axes[0, 2])\n",
    "        axes[0, 2].set_xticklabels(axes[0, 2].get_xticklabels(), rotation=45)\n",
    "        axes[0, 2].set_title('Price Distribution by Category (Top 10)')\n",
    "        axes[0, 2].set_ylabel('Price (USD)')\n",
    "        \n",
    "        # 4. Discount analysis\n",
    "        discount_data = self.df[self.df['discount_pct'] > 0]\n",
    "        axes[1, 0].hist(discount_data['discount_pct'], bins=30, alpha=0.7, edgecolor='black')\n",
    "        axes[1, 0].set_xlabel('Discount Percentage')\n",
    "        axes[1, 0].set_ylabel('Frequency')\n",
    "        axes[1, 0].set_title('Discount Distribution')\n",
    "        axes[1, 0].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 5. Price segments\n",
    "        segment_counts = self.df['price_segment'].value_counts()\n",
    "        axes[1, 1].pie(segment_counts.values, labels=segment_counts.index, autopct='%1.1f%%')\n",
    "        axes[1, 1].set_title('Price Segment Distribution')\n",
    "        \n",
    "        # 6. Brand price analysis (top 10 brands)\n",
    "        top_brands = self.df['brand_clean'].value_counts().head(10).index\n",
    "        brand_prices = self.df[self.df['brand_clean'].isin(top_brands)]\n",
    "        brand_avg_prices = brand_prices.groupby('brand_clean')['price_final_usd'].mean().sort_values(ascending=False)\n",
    "        \n",
    "        axes[1, 2].barh(range(len(brand_avg_prices)), brand_avg_prices.values)\n",
    "        axes[1, 2].set_yticks(range(len(brand_avg_prices)))\n",
    "        axes[1, 2].set_yticklabels(brand_avg_prices.index)\n",
    "        axes[1, 2].set_xlabel('Average Price (USD)')\n",
    "        axes[1, 2].set_title('Average Price by Brand (Top 10)')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # Interactive plotly chart for price distribution\n",
    "        fig = px.box(self.df, x='main_category', y='price_final_usd', \n",
    "                     title=\"Price Distribution Across Categories\")\n",
    "        fig.update_layout(xaxis_tickangle=-45, showlegend=False)\n",
    "        fig.show()\n",
    "    \n",
    "    def brand_analysis(self):\n",
    "        \"\"\"Analyze brand distribution and characteristics\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"BRAND ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        brand_stats = self.df.groupby('brand_clean').agg({\n",
    "            'price_final_usd': ['count', 'mean', 'std', 'min', 'max'],\n",
    "            'discount_pct': 'mean',\n",
    "            'stock_quantity': 'sum'\n",
    "        }).round(2)\n",
    "        \n",
    "        brand_stats.columns = ['_'.join(col).strip() for col in brand_stats.columns.values]\n",
    "        brand_stats = brand_stats.sort_values('price_final_usd_count', ascending=False)\n",
    "        \n",
    "        print(\"Top 10 Brands by Product Count:\")\n",
    "        display(brand_stats.head(10))\n",
    "        \n",
    "        # Brand analysis visualizations\n",
    "        fig, axes = plt.subplots(2, 2, figsize=(15, 12))\n",
    "        \n",
    "        # 1. Brand distribution (top 20)\n",
    "        top_brands = brand_stats.head(20)\n",
    "        axes[0, 0].barh(range(len(top_brands)), top_brands['price_final_usd_count'])\n",
    "        axes[0, 0].set_yticks(range(len(top_brands)))\n",
    "        axes[0, 0].set_yticklabels(top_brands.index)\n",
    "        axes[0, 0].set_xlabel('Number of Products')\n",
    "        axes[0, 0].set_title('Top 20 Brands by Product Count')\n",
    "        \n",
    "        # 2. Price vs product count scatter\n",
    "        axes[0, 1].scatter(brand_stats['price_final_usd_count'], \n",
    "                          brand_stats['price_final_usd_mean'], \n",
    "                          alpha=0.6, s=50)\n",
    "        axes[0, 1].set_xlabel('Number of Products')\n",
    "        axes[0, 1].set_ylabel('Average Price (USD)')\n",
    "        axes[0, 1].set_title('Brand Portfolio: Size vs Price Positioning')\n",
    "        axes[0, 1].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 3. Discount strategy by brand\n",
    "        high_discount_brands = brand_stats[brand_stats['discount_pct_mean'] > 10].head(15)\n",
    "        axes[1, 0].barh(range(len(high_discount_brands)), \n",
    "                       high_discount_brands['discount_pct_mean'])\n",
    "        axes[1, 0].set_yticks(range(len(high_discount_brands)))\n",
    "        axes[1, 0].set_yticklabels(high_discount_brands.index)\n",
    "        axes[1, 0].set_xlabel('Average Discount (%)')\n",
    "        axes[1, 0].set_title('Top 15 Brands by Average Discount')\n",
    "        \n",
    "        # 4. Stock distribution by brand\n",
    "        high_stock_brands = brand_stats.nlargest(15, 'stock_quantity_sum')\n",
    "        axes[1, 1].barh(range(len(high_stock_brands)), \n",
    "                       high_stock_brands['stock_quantity_sum'])\n",
    "        axes[1, 1].set_yticks(range(len(high_stock_brands)))\n",
    "        axes[1, 1].set_yticklabels(high_stock_brands.index)\n",
    "        axes[1, 1].set_xlabel('Total Stock Quantity')\n",
    "        axes[1, 1].set_title('Top 15 Brands by Stock Quantity')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # Interactive brand analysis\n",
    "        fig = px.scatter(brand_stats.reset_index(), \n",
    "                        x='price_final_usd_count', \n",
    "                        y='price_final_usd_mean',\n",
    "                        size='stock_quantity_sum',\n",
    "                        color='discount_pct_mean',\n",
    "                        hover_name='brand_clean',\n",
    "                        title='Brand Analysis: Portfolio Size vs Price Positioning',\n",
    "                        labels={\n",
    "                            'price_final_usd_count': 'Number of Products',\n",
    "                            'price_final_usd_mean': 'Average Price (USD)',\n",
    "                            'discount_pct_mean': 'Average Discount (%)',\n",
    "                            'stock_quantity_sum': 'Total Stock'\n",
    "                        })\n",
    "        fig.show()\n",
    "    \n",
    "    def category_analysis(self):\n",
    "        \"\"\"Analyze category distribution and pricing\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"CATEGORY ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        category_stats = self.df.groupby('main_category').agg({\n",
    "            'price_final_usd': ['count', 'mean', 'std', 'min', 'max'],\n",
    "            'discount_pct': 'mean',\n",
    "            'stock_quantity': 'sum',\n",
    "            'brand_clean': 'nunique'\n",
    "        }).round(2)\n",
    "        \n",
    "        category_stats.columns = ['_'.join(col).strip() for col in category_stats.columns.values]\n",
    "        category_stats = category_stats.sort_values('price_final_usd_count', ascending=False)\n",
    "        \n",
    "        print(\"Category Statistics:\")\n",
    "        display(category_stats)\n",
    "        \n",
    "        # Category analysis visualizations\n",
    "        fig, axes = plt.subplots(2, 2, figsize=(15, 12))\n",
    "        \n",
    "        # 1. Category distribution\n",
    "        top_categories = category_stats.head(15)\n",
    "        axes[0, 0].barh(range(len(top_categories)), top_categories['price_final_usd_count'])\n",
    "        axes[0, 0].set_yticks(range(len(top_categories)))\n",
    "        axes[0, 0].set_yticklabels(top_categories.index)\n",
    "        axes[0, 0].set_xlabel('Number of Products')\n",
    "        axes[0, 0].set_title('Top 15 Categories by Product Count')\n",
    "        \n",
    "        # 2. Average price by category\n",
    "        price_by_category = category_stats.nlargest(15, 'price_final_usd_mean')\n",
    "        axes[0, 1].barh(range(len(price_by_category)), price_by_category['price_final_usd_mean'])\n",
    "        axes[0, 1].set_yticks(range(len(price_by_category)))\n",
    "        axes[0, 1].set_yticklabels(price_by_category.index)\n",
    "        axes[0, 1].set_xlabel('Average Price (USD)')\n",
    "        axes[0, 1].set_title('Top 15 Categories by Average Price')\n",
    "        \n",
    "        # 3. Discount levels by category\n",
    "        discount_by_category = category_stats.nlargest(15, 'discount_pct_mean')\n",
    "        axes[1, 0].barh(range(len(discount_by_category)), discount_by_category['discount_pct_mean'])\n",
    "        axes[1, 0].set_yticks(range(len(discount_by_category)))\n",
    "        axes[1, 0].set_yticklabels(discount_by_category.index)\n",
    "        axes[1, 0].set_xlabel('Average Discount (%)')\n",
    "        axes[1, 0].set_title('Top 15 Categories by Average Discount')\n",
    "        \n",
    "        # 4. Brands per category\n",
    "        brands_per_category = category_stats.nlargest(15, 'brand_clean_nunique')\n",
    "        axes[1, 1].barh(range(len(brands_per_category)), brands_per_category['brand_clean_nunique'])\n",
    "        axes[1, 1].set_yticks(range(len(brands_per_category)))\n",
    "        axes[1, 1].set_yticklabels(brands_per_category.index)\n",
    "        axes[1, 1].set_xlabel('Number of Brands')\n",
    "        axes[1, 1].set_title('Top 15 Categories by Brand Diversity')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # Interactive category analysis\n",
    "        fig = px.treemap(category_stats.reset_index(),\n",
    "                        path=['main_category'],\n",
    "                        values='price_final_usd_count',\n",
    "                        color='price_final_usd_mean',\n",
    "                        hover_data=['discount_pct_mean', 'brand_clean_nunique'],\n",
    "                        title='Category Analysis TreeMap',\n",
    "                        color_continuous_scale='Blues')\n",
    "        fig.show()\n",
    "    \n",
    "    def discount_analysis(self):\n",
    "        \"\"\"Analyze discount patterns and strategies\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"DISCOUNT ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        discount_products = self.df[self.df['has_discount'] == True]\n",
    "        no_discount_products = self.df[self.df['has_discount'] == False]\n",
    "        \n",
    "        print(f\"Products with discounts: {len(discount_products):,} ({len(discount_products)/len(self.df)*100:.1f}%)\")\n",
    "        print(f\"Products without discounts: {len(no_discount_products):,} ({len(no_discount_products)/len(self.df)*100:.1f}%)\")\n",
    "        \n",
    "        # Discount analysis visualizations\n",
    "        fig, axes = plt.subplots(2, 2, figsize=(15, 12))\n",
    "        \n",
    "        # 1. Discount distribution\n",
    "        axes[0, 0].hist(discount_products['discount_pct'], bins=30, alpha=0.7, edgecolor='black')\n",
    "        axes[0, 0].set_xlabel('Discount Percentage')\n",
    "        axes[0, 0].set_ylabel('Frequency')\n",
    "        axes[0, 0].set_title('Discount Distribution (Products with Discounts)')\n",
    "        axes[0, 0].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 2. Discount by price segment\n",
    "        discount_by_segment = self.df.groupby('price_segment')['has_discount'].mean() * 100\n",
    "        axes[0, 1].bar(discount_by_segment.index, discount_by_segment.values)\n",
    "        axes[0, 1].set_xlabel('Price Segment')\n",
    "        axes[0, 1].set_ylabel('Percentage with Discount (%)')\n",
    "        axes[0, 1].set_title('Discount Prevalence by Price Segment')\n",
    "        axes[0, 1].tick_params(axis='x', rotation=45)\n",
    "        \n",
    "        # 3. Discount vs price correlation\n",
    "        axes[1, 0].scatter(discount_products['price_final_usd'], \n",
    "                          discount_products['discount_pct'], \n",
    "                          alpha=0.5, s=20)\n",
    "        axes[1, 0].set_xlabel('Price (USD)')\n",
    "        axes[1, 0].set_ylabel('Discount Percentage')\n",
    "        axes[1, 0].set_title('Discount vs Price Correlation')\n",
    "        axes[1, 0].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 4. Discount by category\n",
    "        discount_by_category = self.df.groupby('main_category')['discount_pct'].mean().nlargest(10)\n",
    "        axes[1, 1].barh(range(len(discount_by_category)), discount_by_category.values)\n",
    "        axes[1, 1].set_yticks(range(len(discount_by_category)))\n",
    "        axes[1, 1].set_yticklabels(discount_by_category.index)\n",
    "        axes[1, 1].set_xlabel('Average Discount (%)')\n",
    "        axes[1, 1].set_title('Top 10 Categories by Average Discount')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # Statistical analysis of discounts\n",
    "        print(\"\\nDiscount Statistics:\")\n",
    "        discount_stats = discount_products['discount_pct'].describe()\n",
    "        display(pd.DataFrame(discount_stats).T)\n",
    "        \n",
    "        # Price comparison: discounted vs non-discounted\n",
    "        print(\"\\nPrice Comparison: Discounted vs Non-Discounted Products:\")\n",
    "        price_comparison = self.df.groupby('has_discount')['price_final_usd'].agg(['mean', 'median', 'std'])\n",
    "        display(price_comparison)\n",
    "    \n",
    "    def stock_analysis(self):\n",
    "        \"\"\"Analyze stock levels and inventory patterns\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"STOCK ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        stock_stats = self.df['stock_quantity'].describe()\n",
    "        print(\"Stock Quantity Statistics:\")\n",
    "        display(pd.DataFrame(stock_stats).T)\n",
    "        \n",
    "        # Stock analysis visualizations\n",
    "        fig, axes = plt.subplots(2, 2, figsize=(15, 12))\n",
    "        \n",
    "        # 1. Stock distribution\n",
    "        axes[0, 0].hist(self.df['stock_quantity'], bins=50, alpha=0.7, edgecolor='black')\n",
    "        axes[0, 0].set_xlabel('Stock Quantity')\n",
    "        axes[0, 0].set_ylabel('Frequency')\n",
    "        axes[0, 0].set_title('Stock Quantity Distribution')\n",
    "        axes[0, 0].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 2. Stock vs price\n",
    "        sample_data = self.df.sample(min(1000, len(self.df)))  # Sample for performance\n",
    "        axes[0, 1].scatter(sample_data['price_final_usd'], sample_data['stock_quantity'], alpha=0.5)\n",
    "        axes[0, 1].set_xlabel('Price (USD)')\n",
    "        axes[0, 1].set_ylabel('Stock Quantity')\n",
    "        axes[0, 1].set_title('Stock Quantity vs Price')\n",
    "        axes[0, 1].grid(True, alpha=0.3)\n",
    "        \n",
    "        # 3. Stock by price segment\n",
    "        stock_by_segment = self.df.groupby('price_segment')['stock_quantity'].mean()\n",
    "        axes[1, 0].bar(stock_by_segment.index, stock_by_segment.values)\n",
    "        axes[1, 0].set_xlabel('Price Segment')\n",
    "        axes[1, 0].set_ylabel('Average Stock Quantity')\n",
    "        axes[1, 0].set_title('Average Stock by Price Segment')\n",
    "        axes[1, 0].tick_params(axis='x', rotation=45)\n",
    "        \n",
    "        # 4. Top categories by total stock\n",
    "        stock_by_category = self.df.groupby('main_category')['stock_quantity'].sum().nlargest(10)\n",
    "        axes[1, 1].barh(range(len(stock_by_category)), stock_by_category.values)\n",
    "        axes[1, 1].set_yticks(range(len(stock_by_category)))\n",
    "        axes[1, 1].set_yticklabels(stock_by_category.index)\n",
    "        axes[1, 1].set_xlabel('Total Stock Quantity')\n",
    "        axes[1, 1].set_title('Top 10 Categories by Total Stock')\n",
    "        \n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
    "        # Low stock analysis\n",
    "        low_stock_threshold = self.df['stock_quantity'].quantile(0.25)\n",
    "        low_stock_products = self.df[self.df['stock_quantity'] <= low_stock_threshold]\n",
    "        \n",
    "        print(f\"\\nLow Stock Analysis (Bottom 25%):\")\n",
    "        print(f\"Low stock threshold: {low_stock_threshold}\")\n",
    "        print(f\"Products with low stock: {len(low_stock_products):,}\")\n",
    "        print(f\"Average price of low stock products: ${low_stock_products['price_final_usd'].mean():.2f}\")\n",
    "        print(f\"Average discount of low stock products: {low_stock_products['discount_pct'].mean():.1f}%\")\n",
    "    \n",
    "    def correlation_analysis(self):\n",
    "        \"\"\"Analyze correlations between different variables\"\"\"\n",
    "        print(\"=\" * 50)\n",
    "        print(\"CORRELATION ANALYSIS\")\n",
    "        print(\"=\" * 50)\n",
    "        \n",
    "        # Select numerical columns for correlation analysis\n",
    "        numerical_cols = ['price_final_usd', 'discount_pct', 'stock_quantity']\n",
    "        \n",
    "        # Create correlation matrix\n",
    "        corr_matrix = self.df[numerical_cols].corr()\n",
    "        \n",
    "        # Plot correlation heatmap\n",
    "        plt.figure(figsize=(8, 6))\n",
    "        sns.heatmap(corr_matrix, annot=True, cmap='coolwarm', center=0,\n",
    "                   square=True, fmt='.2f', cbar_kws={'shrink': .8})\n",
    "        plt.title('Correlation Matrix: Price, Discount, and Stock')\n",
    "        plt.tight_layout()\n",
    "        plt.show()\n",
    "        \n",
